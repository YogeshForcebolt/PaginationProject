@{
    ViewData["Title"] = "Home Page";
}

@model List<Employee>

<div class="container">
    <div class="row">
        <div class="col-sm">
            <h6>Id</h6>
        </div>
        <div class="col-sm">
            <h6>Name</h6>
        </div>
        <div class="col-sm">
            <h6>Age</h6>
        </div>
        <div class="col-sm">
            <h6>Gender</h6>
        </div>
        <div class="col-sm">
            <h6>Status</h6>
        </div>
        <div class="col-sm">
            <h6>Created Date</h6>
        </div>
        <div class="col-sm">
            <h6>Modified Date</h6>
        </div>
    </div>
    @foreach (var i in Model)
    {
        <div class="row">
            <div class="col-sm">
                @i.Id
            </div>
            <div class="col-sm">
                @i.Name
            </div>
            <div class="col-sm">
                @i.Gender
            </div>
            <div class="col-sm">
                @i.Age
            </div>
            <div class="col-sm">
                @i.Status
            </div>
            <div class="col-sm">
                @i.modifed_date
            </div>
            <div class="col-sm">
                @i.created_date
            </div>
        </div>
    }
    
    <nav aria-label="Page navigation example">
        <ul class="pagination">
            <li class="page-item"><a class="page-link" href="#">Previous</a></li>
            <li class="page-item"><a class="page-link" href="#">1</a></li>
            <li class="page-item"><a class="page-link" href="/home/index?next=2">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item"><a class="page-link" href="#">Next</a></li>
        </ul>
    </nav>

</div>


